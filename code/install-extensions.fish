#!/usr/bin/env fish

# Bootstrap 3 Snippets
code --install-extension wcwhitehead.bootstrap-3-snippets
# C/C++
code --install-extension ms-vscode.cpptools
# Code Outline
code --install-extension patrys.vscode-code-outline
# Fish shell
code --install-extension TeddyDD.fish
# FontSize Shortcuts
code --install-extension peterjuras.fontsize-shortcuts
# gitignore
code --install-extension codezombiech.gitignore
# IntelliSense for CSS class names
code --install-extension Zignd.html-css-class-completion
# LaTeX Workshop
code --install-extension James-Yu.latex-workshop
# Nomo Dark Icon Theme
code --install-extension be5invis.vscode-icontheme-nomo-dark
# TODO Highlight
code --install-extension wayou.vscode-todo-highlight
# vimL
code --install-extension fallenwood.viml

echo "vscode extensions install script complete"
echo ""
