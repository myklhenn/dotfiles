# normal text
# [[ NO COLOR ]]
NORMAL 00
# regular file
# [[ NO COLOR ]]
FILE 00
# reset color
# [[ NO COLOR ]]
RESET 00
# directory
# [[ BLUE, BOLD ]]
DIR 34;01
# symbolic link
# (if you set this to 'target' instead of a numerical value, the color is as 
# for the file pointed to)
# [[ CYAN, BOLD ]]
LINK 36;01
# multi-hard-linked file
# [[ DARK-GREY BACKGROUND ]]
MULTIHARDLINK 100
# pipe
# [[ ORANGE, DARK-GREY BACKGROUND ]]
FIFO 33;100
# socket
# [[ PURPLE, BOLD ]]
SOCK 35;01
# door
# [[ PURPLE, BOLD ]]
DOOR 35;01
# block device driver
# [[ ORANGE, BOLD, DARK-GREY BACKGROUND ]]
BLK 33;01;100
# character device driver
# [[ ORANGE, BOLD, DARK-GREY BACKGROUND ]]
CHR 33;01;100
# symlink to nonexistent file, or non-stat'able file
# [[ BLACK, CYAN BACKGROUND ]]
ORPHAN 30;46
# .. and the files they point to
# [[ BLACK, BOLD ]]
MISSING 30;01
# file that is setuid (u+s)
# [[ GREY, RED BACKGROUND ]]
SETUID 37;41
# file that is setgid (g+s)
# [[ BLACK, ORANGE BACKGROUND ]]
SETGID 30;43
# file with capability
# [[ BLACK, RED BACKGROUND ]]
CAPABILITY 30;41
# dir that is sticky and other-writable (+t,o+w)
# [[ BLACK, GREEN BACKGROUND ]]
STICKY_OTHER_WRITABLE 30;42
# dir that is other-writable (o+w) and not sticky
# [[ BLACK, BLUE BACKGROUND ]]
OTHER_WRITABLE 30;44
# dir with the sticky bit set (+t) and not other-writable
# [[ GREY, BLUE BACKGROUND ]]
STICKY 37;44
# file with execute permission
# [[ BOLD, GREEN ]]
EXEC 01;32